<?xml version="1.0" encoding="utf-8" ?>
<odoo>
        <record id="ir_cron_send_check_notifier_email" model="ir.cron">
            <field name="name">Send Check Notifier Email</field>
            <field name="model_id" ref="l10n_tr_account_check.model_account_check"/>
            <field name="state">code</field>
            <field name="code">model.send_email_check_notifier()</field>
            <field name="active" eval="True"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="active">True</field>
            <field name="numbercall">-1</field>
        </record>
</odoo>