<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>

        <record id="action_user_session" model="ir.actions.act_window">
            <field name="name">Session</field>
            <field name="res_model">user.session</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p>
                    View Sessions.
                </p>
            </field>
            <field name="context">{
            }</field>
        </record>
        
        <!-- Menu Item -->
        <menuitem id="menu_user_session" name="Session" action="action_user_session" parent="sale.product_menu_catalog" sequence="7"/>

        

        <record model="ir.ui.view" id="user_session_form">
            <field name="name">user_session_form</field> 
            <field name="model">user.session</field>
            <field name="arch" type="xml">
                <field name="user_id"/>
                <field name="login_date"/>
            </field>
        </record>

        <record model="ir.ui.view" id="user_session_tree">
            <field name="name">user_session_tree</field> 
            <field name="model">user.session</field>
            <field name="arch" type="xml">
                <field name="user_id"/>
                <field name="login_date"/>
            </field>
        </record>

