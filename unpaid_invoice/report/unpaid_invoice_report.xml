<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="unpaid_report">
        <t t-call="unpaid_invoice.html_container">
            <t t-call="unpaid_invoice.internal_layout">
                <div class="act_as_table data_table" style="width: 100%;">
                    <t t-foreach="match.values()" t-as="m">
                        <t t-foreach="m.values()" t-as="d">
                            <div class="act_as_row lines">
                                <div class="act_as_cell">
                                    <span>
                                        <t t-raw="d['ref']" />
                                    </span>
                                </div>
                                <div class="act_as_cell">
                                    <span>
                                        <t t-raw="d['partner']" />
                                    </span>
                                </div>
                                <div class="act_as_cell">
                                    <span>
                                        <t t-raw="d['date']" />
                                    </span>
                                </div>
                                <div class="act_as_cell">
                                    <span>
                                        <t t-raw="d['delay']" />
                                    </span>
                                </div>
                            </div>
                        </t>
                        <br></br>
                    </t>
                </div>
                <h1>Hello Report</h1>
            </t>
        </t>
    </template>
</odoo>