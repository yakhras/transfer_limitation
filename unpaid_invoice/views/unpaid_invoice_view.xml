<?xml version="1.0" encoding="utf-8" ?>
<odoo>
<data>
    <record model="ir.ui.view" id="unpaid_invoice_view_tree">
        <field name="name">unpaid_invoice_view_tree</field> <!--View name-->
        <field name="model">account.move</field> <!--Model name where view will applied-->
        <field name="inherit_id" ref="account.view_out_invoice_tree"/> <!--External ID of Parent view which inherited-->
        <field name="arch" type="xml">
            <xpath expr="//tree" position="inside">
                <field name="transfer_limit" /> 
            </xpath>
        </field>
    </record>

    <record id="sale_order_inherited_tree_view" model="ir.ui.view">
           <field name="name">sale.order.view.tree.inherit</field>
           <field name="model">sale.order</field>
           <field name="inherit_id" ref="sale.view_order_tree"/>
           <field name="arch" type="xml">
               <xpath expr="//tree" position="attributes">
                   <attribute name="js_class">button_in_tree</attribute>
               </xpath>
           </field>
       </record>
</data>
</odoo>


       


  