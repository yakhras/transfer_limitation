<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- Sequence Number for Visit-->
    <record id="lugano_point_sequence" model="ir.sequence">
        <field name="name">Lugano Point</field>
        <field name="code">lugano.survey</field>
        <field name="prefix">PNT-</field>
        <field name="padding">3</field>
    </record>

    <!-- Sequence Number for Visit-->
    <record id="lugano_visit_sequence" model="ir.sequence">
        <field name="name">Lugano Visit</field>
        <field name="code">lugano.visit</field>
        <field name="prefix">VST-</field>
        <field name="padding">3</field>
    </record>

</odoo>