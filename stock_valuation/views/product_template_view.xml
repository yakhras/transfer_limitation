<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record model="ir.ui.view" id="product_template_form">
        <field name="name">product_template_form</field> <!--View name-->
        <field name="model">product.template</field> <!--Model name where view will applied-->
        <field name="inherit_id" ref="product.product_template_form_view"/> <!--External ID of Parent view which inherited-->
        <field name="arch" type="xml">
            <field name="categ_id" position="after"> <!--existing field-->
                <field name="result"/>
            </field>
        </field>
    </record>
</odoo>
