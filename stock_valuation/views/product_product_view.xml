<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record model="ir.ui.view" id="product_product_form">
        <field name="name">product_product_form</field> <!--View name-->
        <field name="model">product.product</field> <!--Model name where view will applied-->
        <field name="inherit_id" ref="product.product_normal_form_view"/> <!--External ID of Parent view which inherited-->
        <field name="arch" type="xml">
            <xpath expr="//page[last()]" position="after">
                <page name="cost" string="Cost">
                    <field name="result">
                    </field>
                </page>
            </xpath>
        </field>
    </record>
</odoo>
